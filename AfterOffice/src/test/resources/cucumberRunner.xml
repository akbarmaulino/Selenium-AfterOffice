<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!-- parallel sama thread count dibuat biar bisa dirun dibeda thread -->
<suite name="CucumberSuite" verbose="1" parallel="true" thread-count="5">
  <test name="Cucumber Test">
    <classes>
       <class name="cucumber.runner.cucumberRunner"/>
    </classes>
  </test>
</suite>